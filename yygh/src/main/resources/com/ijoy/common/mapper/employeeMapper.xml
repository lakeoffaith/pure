<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ijoy.common.mapper.EmployeeMapper">
	<resultMap id="employeeResult" type="Employee">
		<id column="id" property="id" />
		<result column="name" property="name"/>
		<result column="password" property="password" />
		<result column="email" property="email" />
		<result column="realName" property="realName" />
		<result column="cellphone" property="cellphone"  />
		<result column="homePath" property="homePath" />
		<result column="idCardNo" property="idCardNo" />
		<result column="healthInsuranceCardType" property="healthInsuranceCardType"  />
		<result column="healthInsuranceCardNo" property="healthInsuranceCardNo"  />
	</resultMap>






</mapper>
